      <zeebe:userTaskForm id="userTaskForm_04hljfi">{
  "components": [
    {
      "text": "# Throw dice\n\nThrow a dice with 20 sides and submit the result.",
      "type": "text",
      "id": "Field_0wtbe5v"
    },
    {
      "label": "Result",
      "type": "number",
      "id": "Field_1dl7l26",
      "key": "result",
      "validate": {
        "required": true,
        "min": 1,
        "max": 20
      }
    },
    {
      "action": "submit",
      "label": "Submit result",
      "type": "button",
      "id": "Field_0yuauyj",
      "key": "field_021j8b1"
    }
  ],
  "type": "default",
  "id": "SubmitD20",
  "executionPlatform": "Camunda Cloud",
  "executionPlatformVersion": "8.1.0",
  "exporter": {
    "name": "Camunda Modeler",
    "version": "5.6.0"
  },
  "schemaVersion": 5
}</zeebe:userTaskForm>
      <zeebe:userTaskForm id="userTaskForm_09ot8g9">{
  "components": [
    {
      "text": "# Your speech is ready",
      "type": "text",
      "id": "Field_0t9x9vg"
    },
    {
      "label": "Speech",
      "type": "textfield",
      "id": "Field_1wlpooj",
      "key": "speech",
      "disabled": true
    },
    {
      "values": [
        {
          "label": "Add another sentence",
          "value": "no"
        },
        {
          "label": "Wrap it up",
          "value": "yes"
        }
      ],
      "type": "radio",
      "id": "Field_0xu1z8n",
      "key": "accepted",
      "defaultValue": "no"
    },
    {
      "action": "submit",
      "label": "Continue",
      "type": "button",
      "id": "Field_1qu96z1",
      "key": "field_0pxyq38"
